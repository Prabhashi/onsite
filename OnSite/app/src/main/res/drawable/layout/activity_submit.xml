<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.madhuri.app1.Submit">



    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton4"
        android:minHeight="15pt"
        android:minWidth="15pt"
        android:background="@drawable/menu"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Reporting Bugs"
        android:textSize="10pt"
        android:background="#00eeee"
        android:textColor="#ff0000"
        android:id="@+id/textView13"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/imageButton4"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignBottom="@+id/imageButton4" />

    <TextView
        android:layout_width="20pt"
        android:layout_height="80pt"
        android:text=""
        android:textSize="9pt"
        android:background="#000000"
        android:id="@+id/textView12"
        android:layout_alignParentTop="false"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="15dp"
        android:layout_alignRight="@+id/textView13"
        android:layout_below="@+id/imageButton4" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Home"
        android:textColor="#ffffff"
        android:textSize="9pt"
        android:id="@+id/textView14"
        android:layout_alignTop="@+id/textView12"
        android:layout_alignLeft="@+id/textView13"
        android:layout_alignStart="@+id/textView13"
        android:layout_marginTop="48dp"
        android:layout_alignRight="@+id/textView12"
        android:layout_alignEnd="@+id/textView12" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Search Project"
        android:textColor="#ffffff"
        android:textSize="9pt"
        android:id="@+id/textView15"
        android:layout_below="@+id/textView14"
        android:layout_toRightOf="@+id/imageButton4"
        android:layout_marginTop="42dp"
        android:layout_alignRight="@+id/textView12"
        android:layout_alignEnd="@+id/textView12" />

</RelativeLayout>
